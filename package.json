    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-table": "^8.21.2",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^16.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "stripe": "^17.5.0",
    "swiper": "^12.1.0",
    "zod": "^3.24.1"
